{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": [ "src/modulo.js"]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/docdash", 
        "encoding": "utf8",               
        "destination": "./out/",          
        "recurse": true,                  
        "tutorials": "./tutoriales",
        "readme": "./README.md"
    }
}